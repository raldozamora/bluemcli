{
  "name": "bluemachinecli",
  "version": "0.7.18",
  "description": "The command-line interface for bluemachinecli.",
  "type": "module",
  "main": "build/index.js",
  "bin": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluemachineclicli.git"
  },
  "author": "Raldo Zamora",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bluemachinecli/cli/issues"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^14.11.2",
    "@types/tmp": "^0.2.3",
    "tsx": "^3.12.6",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "build": "tsc",
    "execute": "tsx ./src/index.ts",
    "repo:create": "tsx ./src/index.ts --token $TOKEN repo create",
    "snapshot:create": "tsx ./src/index.ts --token $TOKEN snapshot create",
    "repo:list": "tsx ./src/index.ts --token $TOKEN repo list",
    "snapshot:list": "tsx ./src/index.ts --token $TOKEN snapshot list --repo-id=$REPO_ID"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.3.4",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "dedent": "^0.7.0",
    "form-data": "^4.0.0",
    "hyperid": "^3.1.1",
    "ora": "^6.1.2",
    "tmp": "^0.2.1"
  },
  "homepage": "https://github.com/bluemachinecli/cli#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "gaming",
    "gamepad"
  ]
}
